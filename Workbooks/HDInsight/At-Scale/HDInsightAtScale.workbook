          {
            "id": "07cf288a-8a94-4f77-a1fa-01365c6106cc",
            "version": "KqlParameterItem/1.0",
            "name": "Message",
            "type": 1,
            "isRequired": true,
            "query": "where type == 'microsoft.hdinsight/clusters' \r\n| summarize Selected = countif(id in ({Clusters:value})), Total = count()\r\n| extend Selected = iff(Selected > 200, 200, Selected)\r\n| project Message = strcat('# ', Selected, ' / ', Total)",
