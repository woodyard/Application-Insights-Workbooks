            "customWidth": "49",
            "name": "TopTimeSeries"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "AMWMetricsUsageDetails\n| summarize timeseries=sum(DailyTimeseriesCount) by TimeGenerated, MetricName, MetricNamespace\n| project timeseries, TimeGenerated, MetricName",
